<div class="page-header">
  <h1>
    Lista de Inseminações
    <%= link_to 'Novo inseminação', new_inseminacao_path, class: 'btn btn-primary pull-right' %>
  </h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Bovino</th>
      <th>Data</th>
      <th>Inseminador</th>
      <th class="col-xs-3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inseminacoes.each do |inseminacao| %>
      <tr>
        <td><%= inseminacao.bovino.nome %></td>
        <td><%= l inseminacao.data %></td>
        <td><%= inseminacao.inseminador.nome %></td>
        <td>
          <%= link_to 'Editar', edit_inseminacao_path(inseminacao) %> |
          <%= link_to 'Remover', inseminacao, method: :delete, data: { confirm: 'Tem certeza?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
